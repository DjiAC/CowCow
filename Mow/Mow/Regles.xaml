<Window x:Class="Mow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mow"
        mc:Ignorable="d"
        Title="Mow Jow - Règles" Height="768" Width="1024">
    <Grid Margin="0">
        <Grid.Background>
            <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/Rules.jpg"/>
        </Grid.Background>
        <Button x:Name="button" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="144" Click="button_Click_1" Height="138" BorderThickness="0" Margin="436,0,436,10">
            <Button.Background>
                <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/Home_Button.png"/>
            </Button.Background>
        </Button>
        <TextBlock x:Name="titre" Margin="287,100,287,0" TextWrapping="Wrap" Height="100" Width="450" HorizontalAlignment="Center" FontSize="72" TextAlignment="Center" FontFamily="Happy Cow" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" FontWeight="Bold" TextDecorations="{x:Null}" Text="REGLES" Foreground="Black"/>
        <TextBox x:Name="equipe" HorizontalAlignment="Left" Height="46" Margin="683,681,0,0" TextWrapping="Wrap" Text="Coding Farmers" VerticalAlignment="Top" Width="307" FontSize="36" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontStyle="Italic" FontFamily="KBTheFlowerFarm" TextDecorations="{x:Null}" TextAlignment="Center" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="374" Margin="79,205,0,0" VerticalAlignment="Top" Width="236" Source="pack://siteoforigin:,,,/Resources/Regles-1.png"/>
        <Image x:Name="image_Copy" HorizontalAlignment="Center" Height="374" Margin="0,204,0,159" VerticalAlignment="Center" Width="236" Source="pack://siteoforigin:,,,/Resources/Regles-2.png"/>
        <Image x:Name="image_Copy1" HorizontalAlignment="Right" Height="374" Margin="0,205,79,0" VerticalAlignment="Top" Width="236" Source="pack://siteoforigin:,,,/Resources/Regles-3.png"/>

    </Grid>
</Window>
